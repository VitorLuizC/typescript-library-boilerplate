{
  "name": "[libraryName]",
  "version": "0.0.0",
  "description": "",
  "cdn": "dist/index.umd.js",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "jsdelivr": "dist/index.umd.js",
  "umd:main": "dist/index.umd.js",
  "devDependencies": {
    "ava": "^2.1.0",
    "bili": "^4.8.0",
    "rollup-plugin-typescript2": "^0.21.2",
    "ts-node": "^8.3.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^2.0.6",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "doc": "typedoc src/index.ts",
    "test": "yarn test:lint && yarn test:unit",
    "test:lint": "prettier **/*.{ts,tsx,js,jsx,json} --check",
    "test:unit": "ava",
    "lint": "prettier **/*.{ts,tsx,js,jsx,json} --write",
    "build": "bili",
    "prepare": "yarn doc && yarn build && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/[repositoryOwner]/[repositoryName].git"
  },
  "keywords": [
    "typescript",
    "boilerplate",
    "library"
  ],
  "author": {
    "name": "[authorFullName]"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/[repositoryOwner]/[repositoryName]/issues"
  },
  "homepage": "https://github.com/[repositoryOwner]/[repositoryName]#readme",
  "ava": {
    "require": [
      "ts-node/register"
    ],
    "extensions": [
      "ts"
    ],
    "compileEnhancements": false
  },
  "dependencies": {
    "prettier": "^1.18.2"
  }
